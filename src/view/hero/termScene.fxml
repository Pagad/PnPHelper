<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.spells.TermSceneController">
   <children>
      <HBox prefHeight="400.0" prefWidth="480.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="editField" />
                  <Separator prefHeight="13.0" prefWidth="177.0" />
                  <TextArea fx:id="textArea" editable="false" prefHeight="295.0" prefWidth="177.0" />
               </children>
            </VBox>
            <VBox prefHeight="400.0" prefWidth="150.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#save" text="Save" />
                  <Button mnemonicParsing="false" onAction="#goBack" text="goBack" />
               </children>
            </VBox>
            <VBox prefHeight="400.0" prefWidth="200.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
